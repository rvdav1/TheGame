<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.DeleteProfileController">
   <children>
      <Label alignment="CENTER" prefHeight="30.0" text="Delete profile" AnchorPane.leftAnchor="160.0" AnchorPane.rightAnchor="80.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
      <Button fx:id="deleteProfileButton" mnemonicParsing="false" onAction="#handleDeleteProfile" prefHeight="25.0" text="Delete" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="220.0" />
      <Button fx:id="backDel" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleDelBack" prefHeight="25.0" prefWidth="150.0" text="Back" AnchorPane.leftAnchor="260.0" AnchorPane.rightAnchor="180.0" AnchorPane.topAnchor="320.0" />
      <ComboBox fx:id="deleteBox" prefHeight="40.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="80.0" />
      <Button fx:id="deleteAllButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleDeleteAllProfile" prefHeight="25.0" text="Delete All" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="260.0" />
      <Label fx:id="labelDel" alignment="CENTER" contentDisplay="CENTER" textAlignment="CENTER" textFill="RED" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="360.0">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
