<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.OptController">
   <children>
      <Label alignment="CENTER" prefHeight="120.0" text="Set team profiles" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="40.0" />
         </font></Label>
      <Label prefHeight="25.0" prefWidth="80.0" text="1.team" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="120.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="firstOptTeamBox" layoutX="30.0" layoutY="30.0" prefHeight="25.0" prefWidth="150.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="120.0" />
      <Button fx:id="firstOptTeamButton" layoutX="295.0" layoutY="375.0" mnemonicParsing="false" onAction="#handleOptFirstTeam" prefHeight="25.0" text="Set first team" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="170.0" />
      <Label prefHeight="25.0" prefWidth="80.0" text="2.team" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="220.0">
         <font>
            <Font size="20.0" />
         </font></Label>
      <ComboBox fx:id="secondOptTeamBox" layoutX="40.0" layoutY="40.0" prefHeight="25.0" prefWidth="150.0" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="220.0" />
      <Button fx:id="secondOptTeamButton" layoutX="305.0" layoutY="385.0" mnemonicParsing="false" onAction="#handleOptSecondTeam" prefHeight="25.0" text="Set second team" AnchorPane.leftAnchor="120.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="270.0" />
      <Button fx:id="newOptProfile" layoutX="315.0" layoutY="395.0" mnemonicParsing="false" onAction="#handleOptNewProfile" prefHeight="25.0" text="Add new profile" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="120.0" AnchorPane.topAnchor="320.0" />
      <Button fx:id="backOpt" layoutX="325.0" layoutY="405.0" mnemonicParsing="false" onAction="#handleOptBack" prefHeight="25.0" text="Back" AnchorPane.leftAnchor="280.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="400.0" />
      <Label fx:id="labelOpt" alignment="CENTER" layoutX="331.0" layoutY="439.0" textAlignment="CENTER" textFill="RED" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="430.0">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button fx:id="deleteOptProfile" layoutX="325.0" layoutY="405.0" mnemonicParsing="false" onAction="#handleOptDeleteProfile" prefHeight="25.0" text="Delete profile" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="120.0" AnchorPane.topAnchor="360.0" />
   </children>
</AnchorPane>
